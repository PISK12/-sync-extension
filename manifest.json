{
	"manifest_version": 2,
	"name": "sync",
	"version": "1.0.3",
	"description": "Sync between device",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"/content_script.js"
			]
		}
	],
	"browser_action": {
		"default_title": "Sync",
		"default_popup": "/popup.html"
	},
	"permissions": [
		"tabs", "activeTab","storage","webRequest"
	]
}